[Variables]
%ProgramTitle%=Norton Power Eraser
%ProgramExe%=NPE.exe
%ProgramFolder%=Norton Power Eraser
%SetupURL%=https://support.norton.com/sp/static/external/NPE/5.3/prod/en/NPE.exe

[Saved]
%SetupURL%=http://liveupdate.symantec.com/upgrade/NPE/1033/NPE.exe

[Process]
Echo,"Processing %ScriptTitle%..."
If,%Architecture%%WoW64Support%,Equal,x64False,EchoExtended,"You Need To Enable WoW64 Basic To Run %ProgramTitle% In A 64-Bit Build",,,Message,2,Exit
Iniread,%ProjectDir%\Script.Project,Features,Msi,%Msi%
//If,%Msi%,Equal,False,EchoExtended,"You Need To Enable > MSI Windows Installer < In Build Core To Run %ProgramTitle%",,,Message,4,Exit
If,%Msi%,Equal,False,Exec,%ProjectDir%\Features\Msi.Script,Process
If,%RunFromWhere_ScrollBox%,Equal,"Run From RAM",RunFromRAM
//--
DirMake,%Target_Prog%\%ProgramFolder%
Download,%Target_Prog%\%ProgramFolder%\%ProgramExe%,%SetupURL%
If,Not,ExistFile,%Target_Prog%\%ProgramFolder%\%ProgramExe%,EchoExtended,"Error: %SetupFile% Has Not Been Downloaded.",Warn,,Message,5,Exit
//--
ExtractSectionFiles,%ScriptFile%,AddFiles
If,%Architecture%,Equal,x64,ExtractSectionFiles,%ScriptFile%,AddFiles6432
//--
If,%Desktop_CheckBox%,Equal,True,AddShortcut,Desktop
If,%StartMenu_CheckBox%,Equal,True,AddShortcut,StartMenu,%StartMenuFolder_TextBox%
If,%StartMpin_CheckBox%,Equal,True,AddPin,StartMenu
If,%TaskBpin_CheckBox%,Equal,True,AddPin,TaskBar

