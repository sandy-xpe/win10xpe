[Variables]
%ProgramTitle%=X-Mouse Button Control
%ProgramExe%=XMouseButtonControl.exe
%ProgramFolder%=X-Mouse Button Control
%SetupFile%=XMouseButtonControl_Setup.exe
%SetupURL%=https://dvps.highrez.co.uk/downloads/XMouseButtonControlSetup.%ProgramVer_TextBox%.exe
%ProvideFiles%=%ProgCache%\%ProgramFolder%
%FileContainer%=%ScriptDir%\XMouseButtonControl_XPE_File.Script

[Process]
Echo,"Processing %ScriptTitle%..."
If,Not,ExistFile,%FileContainer%,Exit,"%FileContainer% Container File Not Found"
If,%RunFromWhere_ScrollBox%,Equal,"Run From RAM",RunFromRAM
//--
Run,%ScriptFile%,Extract
//--
Set,%Tapp%,%Target_Prog%\%ProgramFolder%
Set,%SourceProfile%,%GTemp%\%ProgramFolder%\$APPDATA\Highresolution Enterprises\XMouseButtonControl
//
If,ExistDir,%Tapp%,DirDeleteQ,%Tapp%
If,Not,ExistDir,%Tapp%,DirMake,%Tapp%
If,%Architecture%,Equal,x86,Run,%ScriptFile%,Filesx86
If,%Architecture%,Equal,x64,Run,%ScriptFile%,Filesx64
//--
If,Not,%Lang_ScrollBox%,Equal,English,Begin
If,%Lang_ScrollBox%,Equal,Espanol-LatinoAmerica,FileRename,%SourceProfile%\Español-LatinoAmerica.xmbclp,%SourceProfile%\Espanol-LatinoAmerica.xmbclp
FileCopy,%SourceProfile%\%Lang_ScrollBox%.xmbclp,%Tapp%
FileCopy,%GTemp%\%ProgramFolder%\XMBCSettings.xml,%Tapp%
TXTReplace,%Tapp%\XMBCSettings.xml,<Language filename=#$q#$q />,<Language filename=#$q%Lang_ScrollBox%.xmbclp#$q />
End
//--
If,ExistDir,%GCustom%\XMouseProfiles,Begin
Retrieve,FOLDERSIZE,%GCustom%\XMouseProfiles,%XMouseProfilesSize%
If,%XMouseProfilesSize%,Bigger,1,FileCopy,%GCustom%\XMouseProfiles\*,%Tapp%
End
//--
Run,%ScriptFile%,Add_Shortcuts
//--
ExtractSectionFiles,%ScriptFile%,AddFiles
//--
Run,%ScriptFile%,Add_Registry

[FilesX86]
FileCopy,%GTemp%\%ProgramFolder%\XMouseButtonControl_1.exe,%Tapp%\%ProgramExe%
FileCopy,%GTemp%\%ProgramFolder%\BugTrapU.dll,%Tapp%
FileCopy,%GTemp%\%ProgramFolder%\XMouseButtonHook_1.dll,%Tapp%\XMouseButtonHook.dll
FileCopy,%GTemp%\%ProgramFolder%\X-Mouse Button Control User Guide.pdf,%Tapp%
FileCopy,%GTemp%\%ProgramFolder%\License.txt,%Tapp%
FileCopy,%GTemp%\%ProgramFolder%\uninstaller.exe,%Tapp%

[FilesX64]
