Author=ChrisR
Date=2019.03.31
Credits=TheOven.org Forum
Contact=
Version=003
Level=5
Download_Level=2
Selected=False
Mandatory=False
NoWarning=False
History001=ChrisR Initial XPE Packed Plugin Created 2019.03.07
History003=KYHI - M&M Check - 2019.03.31

[Interface]
Shortcut_Bevel=Shortcut_Bevel,1,12,4,6,133,121
Desktop_CheckBox="Desktop Shortcut",1,3,10,10,122,18,False,"__Add Desktop Shortcut"
StartMenu_CheckBox="Start Menu Shortcut",1,3,10,28,122,18,True,"__Add Start Menu Shortcut"
TaskBpin_CheckBox="Pin To TaskBar",1,3,10,46,122,18,False,"__Add Pin To Taskbar Shortcut"
StartMpin_CheckBox="Pin To StartMenu",1,3,10,64,122,18,False,"__Add Pin To StartMenu Shortcut"
StartMenuFolder_TextBox="Start Menu Folder:",1,0,10,100,121,18,,"__Start Menu Folder Name"
RunFromWhere_ScrollBox="Run From USB",1,4,145,6,104,21,"Run From USB","Run From RAM"
Image_Software=diskwipe_thumb1.jpg,1,5,145,35,220,173
Button_Launch_Program=Launch,1,8,10,142,119,25,Launch_Program,Launch.bmp,False,"__Click Here To Launch Program"
HomePage_WebLabel="Home Page",1,10,338,6,55,18,http://www.diskwipe.org/

[Variables]
%ProgramTitle%=DiskWipe
%ProgramExe%=DiskWipe.exe
%ProgramFolder%=DiskWipe
%ProvideFiles%=%ProgCache%\%ProgramFolder%
%SetupURL%=http://www.diskwipe.org/download/DiskWipe.exe
%FileContainer%=%ScriptDir%\DiskWipe_XPE_File.Script

[Process]
Echo,"Processing %ScriptTitle%..."
If,Not,ExistFile,%FileContainer%,Exit,"%FileContainer% Container File Not Found"
If,%Architecture%%WoW64Support%,Equal,x64False,EchoExtended,"You Need To Enable WoW64 Basic To Run %ProgramTitle% In A 64-Bit Build",,,Message,2,Exit
If,%RunFromWhere_ScrollBox%,Equal,"Run From RAM",RunFromRAM
//--
Run,%ScriptFile%,Extract
//--
If,ExistDir,%Target_Prog%\%ProgramFolder%,DirDeleteQ,%Target_Prog%\%ProgramFolder%
DirMake,%Target_Prog%\%ProgramFolder%
FileCopy,%GTemp%\%ProgramFolder%\%ProgramExe%,%Target_Prog%\%ProgramFolder%
//--
If,%Architecture%,Equal,x86,ExtractSectionFiles,%ScriptFile%,DiskWipe_AddFiles
If,%Architecture%,Equal,x64,ExtractSectionFiles,%ScriptFile%,DiskWipe_AddFiles6432
//--
Run,%ScriptFile%,Add_Shortcuts

[DiskWipe_AddFiles]
\Windows\System32\format.com
\Windows\System32\fsutilext.dll
\Windows\System32\ifsutil.dll
\Windows\System32\uexfat.dll
\Windows\System32\ufat.dll
\Windows\System32\untfs.dll
\Windows\System32\user32.dll
\Windows\System32\??-??\user32.dll.mui

[DiskWipe_AddFiles6432]
\Windows\SysWOW64\format.com
\Windows\SysWOW64\fsutilext.dll
\Windows\SysWOW64\ifsutil.dll
\Windows\SysWOW64\uexfat.dll
\Windows\SysWOW64\ufat.dll
\Windows\SysWOW64\untfs.dll
\Windows\SysWOW64\user32.dll
\Windows\SysWOW64\??-??\user32.dll.mui

[Add_Shortcuts]
If,%Desktop_CheckBox%,Equal,True,AddShortcut,Desktop
If,%StartMenu_CheckBox%,Equal,True,AddShortcut,StartMenu,%StartMenuFolder_TextBox%
If,%StartMpin_CheckBox%,Equal,True,AddPin,StartMenu
If,%TaskBpin_CheckBox%,Equal,True,AddPin,TaskBar

[Extract]
Set,%SetupContainerSize%,""
IniRead,%FileContainer%,Folder,%ProgramExe%,%SetupContainerSize%
If,%SetupContainerSize%-,Equal,-,Run,%ScriptFile%,Update_Container
//--
Echo,"Extracting %ScriptTitle% Setup File..."
If,ExistDir,%GTemp%\%ProgramFolder%,DirDeleteQ,%GTemp%\%ProgramFolder%
DirMake,%GTemp%\%ProgramFolder%
ExtractFile,%FileContainer%,Folder,%ProgramExe%,%GTemp%\%ProgramFolder%

[Launch_Program]
Run,%ScriptFile%,Extract
Start,,%ProgramExe%,,%GTemp%\%ProgramFolder%

[Download]
If,ExistDir,%ProvideFiles%,DirDeleteQ,%ProvideFiles%
DirMake,%ProvideFiles%
Download,%ProvideFiles%\%ProgramExe%,%SetupURL%
If,Not,ExistFile,%ProvideFiles%\%ProgramExe%,EchoExtended,"Error: %ProgramExe% has not been downloaded.",Warn,,Message,10,Exit

[Update_Container]
Echo,"Update %ScriptTitle%..."
Run,%ScriptFile%,Download
Encode,%Filecontainer%,Folder,%ProvideFiles%\%ProgramExe%
//-
Retrieve,FileVersion,%ProvideFiles%\%ProgramExe%,%ProgramVer%
IniWrite,%ScriptFile%,Main,Description,"(v%ProgramVer% x86) %ProgramTitle% is Free, portable Windows application for permanent volume data destruction"
IniWrite,%FileContainer%,Main,Description," %ProgramTitle% Version %ProgramVer% File Container"
//--
DirDeleteQ,%ProvideFiles%
System,ReScanScripts

