[Main]
Title=Internet Favorites
Type=XPEPlugin
Description=Plugin Adds Your Internet Favorites
Author=KYHI
Date=2019.02.25
Credits=TheOven.org Forum
Contact=
Version=002
Level=5
Download_Level=2
Selected=False
Mandatory=False
NoWarning=False
History001=KYHI Initial XPE Plugin Created 2018.05.18
History002=KYHI Sync with Project IE11_Favorites Custom Folder - 2019.02.25

[Variables]
%Pers%=%GCustom%\IE11_Favorites
%FavoF%=%GTarget%\Users\Default\Favorites
%URL_Fixer_AU3%=%GTemp%\URL_Fixer_AU3

[Interface]
GlobalDirectory_Button="Open Project Favorites",1,8,6,30,145,36,globaldirectory,_globaldirectory,False,False,_globaldirectory_,False
FavoritesLocaldirectory_Button="Open Local Favorites",1,8,6,70,145,36,FavoritesLocaldirectory,_FavoritesLocaldirectory_,False
RunFromWhere_ScrollBox="Run From USB",1,4,165,6,104,21,"Run From USB","Run From RAM"
Url_Fixer="Use AU3 URL Fixer",1,3,6,110,240,18,False
pImage1=,1,5,185,42,54,58,

[Process]
Echo,"Processing %ScriptTitle%..."
If,%RunFromWhere_ScrollBox%,Equal,"Run From RAM",RunFromRAM
//--
If,Not,ExistDir,%Pers%,DirMake,%Pers%
If,Not,ExistDir,%FavoF%,DirMake,%FavoF%
//--
Retrieve,FolderSize,%pers%,%sourcedirsize%
If,Not,%sourcedirsize%,Equal,0,Begin
FileCopy,%Pers%\*,%FavoF%
End
//--
If,%Url_Fixer%,Equal,True,Run,%ScriptFile%,urlfixer
//--
ExtractSectionFiles,%ScriptFile%,AddFiles
//--
RegHiveLoad,Tmp_Default,%RegDefault%
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced,StartMenuFavorites,1
RegWrite,HKLM,0x4,Tmp_Default\Software\StartIsBack,StartMenuFavorites,2
RegHiveUnLoad,Tmp_Default

[AddFiles]
\Windows\System32\url.dll
\Windows\System32\??-??\url.dll.mui

[urlfixer]
If,ExistDir,%URL_Fixer_AU3%,DirDelete,%URL_Fixer_AU3%
DirMake,%URL_Fixer_AU3%
ExtractFile,%ScriptFile%,URL_Fixer,URLFixer.AU3,%URL_Fixer_AU3%
If,Not,ExistFile,%URL_Fixer_AU3%\AutoIt3.exe,If,ExistFile,%Gtools%\AU3381.exe,FileCopy,%Gtools%\AU3381.exe,%URL_Fixer_AU3%\AutoIt3.exe
If,ExistFile,%URL_Fixer_AU3%\AutoIt3.exe,Shellexecute,Hide,%URL_Fixer_AU3%\AutoIt3.exe,"URLFixer.au3 subyes #$q%FavoF%\*.url#$q",%URL_Fixer_AU3%
Else,Echo,"AutoIt3.exe NOT Found - URL Fixer Not Processed",Warn

[Globaldirectory]
If,Not,ExistDir,%Pers%,DirMake,%Pers%
OpenDir,%Pers%

[FavoritesLocaldirectory]
RegRead,HKCU,"Software\Microsoft\Windows\CurrentVersion\Explorer\User Shell Folders",Favorites,%FavoritesLocal%
OpenDir,%FavoritesLocal%

