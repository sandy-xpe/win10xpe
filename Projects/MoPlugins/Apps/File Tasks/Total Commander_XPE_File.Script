[Main]
Title=Total Commander - File Container
Type=XPEPlugin
Description=Total Commander Version 9.5.1.0 File Container
Author=KYHI
Date=2019.03.31
Credits=
Contact=
Version=002
Level=0
Download_Level=2
Selected=None
Mandatory=False
NoWarning=False
History001=KYHI Initial XPE AutoUpdate Plugin Created 2018.11.25
History002=KYHI - M&M Check - 2019.03.31

[Interface]
Hide_TextBox="To Hide Plugin From Tree - Edit Container File Plugin [Main] Level=5 to Level=0",1,1,10,60,540,18,8,Bold
Update_TextBox="To Update This Container File - Just Press The Little GREEN Play Button",1,1,10,30,540,18,8,Bold

[Variables]
%ProgramTitle%=Total Commander
%ProgramExe%=totalcmd.exe
%ProgramFolder%=Total Commander
%SetupFile%=tcmd951x32.exe
%SetupFilex64%=tcmd951x64.exe
%File_Plugin%=%ScriptDir%\Total Commander_XPE.Script
%File_Container%=%ScriptDir%\Total Commander_XPE_File.Script
%ProvideFiles%=%ProgCache%\%ProgramFolder%
%SetupURL%=https://totalcommander.ch/win/tcmd951x32.exe
%SetupURLx64%=https://totalcommander.ch/win/tcmd951x64.exe

[Process]
Echo,"Update %ScriptTitle%..."
If,ExistDir,%ProvideFiles%,DirDeleteQ,%ProvideFiles%
DirMake,%ProvideFiles%
//--
Run,%ScriptFile%,Download
//--
Retrieve,FileVersion,%ProvideFiles%\%SetupFile%,%ContainerVer%
//-- Update Container File Attachment
IniWrite,%File_Container%,Main,Description," %ProgramTitle% Version %ContainerVer% File Container"
IniWrite,%File_Container%,Main,Selected,None
IniWrite,%File_Container%,Main,Level,0
//-- Update Plugin File Information
IniWrite,%File_Plugin%,Main,Description,"(v%ContainerVer%)  %ProgramTitle% is a Shareware File Manager for Windows"
//--
DirDeleteQ,%ProvideFiles%
System,ReScanScripts

[Download]
If,Not,ExistDir,%ProvideFiles%,DirMake,%ProvideFiles%
Download,%ProvideFiles%\%SetupFile%,%SetupURL%
Encode,%File_Container%,Folder,%ProvideFiles%\%SetupFile%
//--
Download,%ProvideFiles%\%SetupFilex64%,%SetupURLx64%
Encode,%File_Container%,Folder,%ProvideFiles%\%SetupFilex64%

